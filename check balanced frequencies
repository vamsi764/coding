import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        String str="aabbc";
        // HashMap<Character,Integer> map=new HashMap<>();
        int[] freq=new int[26];
        for(char ch:str.toCharArray())
        {
            freq[ch-'a']++;
        }
        boolean balanced=true;
        int count=-1;
        for(int num:freq)
        {
            if(num==0)
            {
                continue;
            }
            else if(count==-1)
            {
                count=num;
            }
            else if(count!=num)
            {
                balanced=false;
                break;
            }
        }
        
        
        System.out.println(balanced);
        
    }
}
